int main()
{ int i,j,n;
printf("enter the value of n");
scanf("%d",&n);

for(i=1;i<=n;i++){
    char a=65;
    int b=1;
for(j=1;j<=i;j++){
    if(i%2==0){
        printf("%c",a);
        a=a+1;
         }
         else{
            printf("%d",b);
            b=b+1;
         }
      
    }
    printf("\n");
}

    return 0;
}
